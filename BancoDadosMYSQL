2019 -> Criando Banco de Dados MYSQL: CÃ³digo Abaixo
====================================================
create database Dragon;

use Dragon;

create table if not exists Usuario(
id bigint(10) auto_increment,
nome varchar(255),
cpf varchar(255),
email varchar(255),
telefone varchar(255),
primary key(id));

select * from Usuario;
 
insert into Usuario(nome,cpf,email,telefone)values ("Martim","123","email","123456789");
